    <div id="menu1" class="tab-pane fade">
        <table class="table active" cellpadding="2px">
            <thead>
                <tr>
                    <th scope="col"></th>
                    <th scope="col">Placa</th>
                    <th scope="col">Marca</th>
                    <th scope="col">Dueño</th>

                </tr>
            </thead>
            @foreach (var item in ViewBag.tab1)
            {
                <tbody>
                    <tr>
                        <th scope="row"></th>
                        <td>@item.Placa</td>
                        <td>@item.Marca</td>
                        <td>@item.Dueno</td>
                        <td><a  class="btn btn-success" asp-route-id="@item.ID_Vehiculo" asp-route-placa="@item.Placa" asp-route-dueno="@item.Dueno" asp-route-marca="@item.Marca" asp-action="VistaServicios" asp-controller="Home"> Asignar Nuevos Servicios</a> </td>
                    </tr>


                </tbody>
            }
        </table>
